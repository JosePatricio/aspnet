using System;using System.Collections.Generic;using System.Diagnostics;using System.Linq;using System.Transactions;using Negocio.Utilidades;using PersistenciaSigeor;using Negocio.Job;namespace Negocio.Reportes{    public class ReporteConsolidadoNegocio    {        public static List<GET_CONSOLIDADO_EOR_Result> ObtenerConsolidadoEors(string parametros)        {            if (string.IsNullOrEmpty(parametros))                return null;            List<GET_CONSOLIDADO_EOR_Result> result;            try            {                var param = Serializador.DeSerializeEntity<ClaseBasica>(parametros);                if (!string.IsNullOrEmpty(param.Descripcion))                {                    EorEstructuraProcesoNegocio.VerificarEorEstructura(param.Descripcion);                    EorMaquinariaProcesoNegocio.VerificarEorMaquinaria(param.Descripcion);                    EorTransitoProcesoNegocio.VerificarEorTransito(param.Descripcion);                }                TransactionOptions transactionOptions = new TransactionOptions                {                    IsolationLevel = System.Transactions.IsolationLevel.ReadUncommitted                };                using (var transactionScope = new TransactionScope(TransactionScopeOption.Required, transactionOptions))                {                    using (var context = new SigeorEntities())                    {                        result = context.GET_CONSOLIDADO_EOR(param.Descripcion, param.DescripcionUno,                            param.FechaDateTimeStringUno, param.FechaDateTimeStringDos, param.EstadoString).ToList();                    }                    transactionScope.Complete();                }            }            catch (Exception ex)            {                Log.WriteEntry("No se pudo obtener el consolidado de EOR's: " + ex, EventLogEntryType.Error);                throw new Exception("No se pudo obtener el consolidado de EOR's: " + ex.Message);            }            return result;//.Any() ? Serializador.SerializeEntity(result) : null;        }        public static string ObtenerConsolidadoEorsPaginado(string parametros, int pagesize, int pageIndex, out int totalRegistros)        {            if (string.IsNullOrEmpty(parametros))            {                totalRegistros = 0;                return null;            }            List<GET_CONSOLIDADO_EOR_Result> result;            try            {                var param = Serializador.DeSerializeEntity<ClaseBasica>(parametros);                if (!string.IsNullOrEmpty(param.Descripcion))                {                    EorEstructuraProcesoNegocio.VerificarEorEstructura(param.Descripcion);                    EorMaquinariaProcesoNegocio.VerificarEorMaquinaria(param.Descripcion);                    EorTransitoProcesoNegocio.VerificarEorTransito(param.Descripcion);                }                TransactionOptions transactionOptions = new TransactionOptions                {                    IsolationLevel = System.Transactions.IsolationLevel.ReadUncommitted                };                using (var transactionScope = new TransactionScope(TransactionScopeOption.Required, transactionOptions))                {                    using (var context = new SigeorEntities())                    {                        result = context.GET_CONSOLIDADO_EOR(param.Descripcion, param.DescripcionUno,                             param.FechaDateTimeStringUno, param.FechaDateTimeStringDos, param.EstadoString).ToList();                    }                    transactionScope.Complete();                }                totalRegistros = result.Count;                result = result.Skip(pagesize * (pageIndex-1)).Take(pagesize).ToList();            }            catch (Exception ex)            {                Log.WriteEntry("No se pudo obtener el consolidado de EOR's paginado: " + ex, EventLogEntryType.Error);                throw new Exception("No se pudo obtener el consolidado de EOR's paginado: " + ex.Message);            }            return result.Any() ? Serializador.SerializeEntity(result) : null;        }    }}