body {
    width: 100%;
    margin: 0;
    float: left;
    text-align: center;
}

#panel {
    width: 95%;
    margin: 0 auto;
    text-align: center;
}

#menu {
    list-style: none;
    counter-reset: li;
    background: transparent;
    -moz-box-shadow: 0 1px 2px rgba(24,25,25,.2) inset;
    -webkit-box-shadow: 0 1px 2px rgba(24,25,25,.2) inset;
    box-shadow: 0 1px 2px rgba(24,25,25,.2) inset;
    -moz-border-radius: 10px;
    -webkit-border-radius: 10px;
    border-radius: 10px;
    width: 99%;
    height: 100%;
    margin: 0 auto;
    position: relative;
    float: left;
    text-align: center;
    padding-top: 2%;
}

#li {
    background-color: #EAEAEA !important;
    border: 1px solid #337AB7 !important;
}

#menu li {
    position: relative;
    float: left;
    cursor: pointer;
    width: 28.5%;
    height: 230px;
    margin: 8px 0 0 8px;
    color: #337AB7 !important;
    border-radius: 5px;
    /*-webkit-box-shadow: 0px 10px 5px -2.5px rgba(0,0,0,0.70);*/
    position: static;
    padding-left: 2%;
    text-align: center;
    border-color: #337AB7 !important;
    background-color: #EAEAEA !important;
}

    #menu li:hover, #menu li:focus {
        background-image: -webkit-gradient(linear, left top, left bottom, from(rgba(24,25,25,.2)), to(rgba(0,0,0,0)));
        background-image: -webkit-linear-gradient(top, rgba(24,25,25,.2), rgba(24,25,25,0));
        background-image: -moz-linear-gradient(top, rgba(24,25,25,.2), rgba(24,25,25,0));
        background-image: -ms-linear-gradient(top, rgba(224,25,25,.2), rgba(24,25,25,0));
        background-image: -o-linear-gradient(top, rgba(24,25,25,.2), rgba(24,25,25,0));
        background-image: linear-gradient(top, rgba(24,25,25,.2), rgba(24,25,25,0));
        cursor: default;
    }

    #menu li:nth-child(1),
    #menu li:nth-child(2),
    #menu li:nth-child(3) {
        margin-top: 0;
    }

    #menu li:nth-child(1),
    #menu li:nth-child(4),
    #menu li:nth-child(7) {
        margin-left: 0;
    }

    #menu li:nth-child(1) {
        background-color: #EAEAEA !important;
        border: 1px solid #337AB7 !important;
    }

    #menu li:nth-child(2) {
        background-color: #EAEAEA !important;
        border: 1px solid #337AB7 !important;
    }

    #menu li:nth-child(3) {
        background-color: #EAEAEA !important;
        border: 1px solid #337AB7 !important;
    }

    #menu li:nth-child(4) {
        background-color: #EAEAEA !important;
        border: 1px solid #337AB7 !important;
    }

    #menu li:nth-child(5) {
        background-color: #EAEAEA !important;
        border: 1px solid #337AB7 !important;
    }

    #menu li:nth-child(6) {
        background-color: #EAEAEA !important;
        border: 1px solid #337AB7 !important;
    }

#menu .title {
    position: absolute;
    height: 100%;
    width: 100%;
    font: italic bold 1em/120px 'trebuchet MS', Arial, helvetica;
    opacity: 0.7;
    -moz-transition: opacity 0.5s;
    -webkit-transition: opacity 0.5s;
    transition: opacity 0.5s;
    color: #337AB7;
    text-decoration: none;
    font-style: oblique;
}

#menu li:hover .title {
    cursor: default;
}

#menu li .image {
    display: block;
    width: 20%;
    margin: 85px auto 0;
    -moz-transition: margin 0.5s;
    -webkit-transition: margin 0.5s;
    transition: margin 0.5s;
}

    #menu li .image:hover {
        margin: 63px auto 0;
        /*display: inline-block;*/
        border-color: #337AB7 !important;
        border-style: solid;
        border-bottom-left-radius: 5px;
        border-bottom-right-radius: 5px;
        border-top-left-radius: 5px;
        border-top-right-radius: 5px;
        position: relative;
        /*transition-duration: 0.5s;*/
        /*transition-property: transform;*/
    }


#menu li .title {
    display: block;
    width: 100%;
    height: auto;
    font-family: Arial, serif;
    font-size: 25px;
    font-style: normal;
    font-weight: normal;
    line-height: 60px;
    position: static;
    text-decoration: none;
    cursor: pointer;
    color: white;
}

.sub {
    text-decoration: none;
    color: #2968a2 /*#fff*/;
    text-align: left;
    font-style: normal;
}

    .sub:target {
        display: block;
    }

    .sub a {
        font-style: normal;
        text-decoration: none;
        color: #337AB7 !important /*#fff*/;
        list-style-type: circle;
        font-size: medium;
        padding-left: 0px;
    }

        .sub a:hover {
            font-style: normal;
            color: #337AB7 !important /*#44b2f1*/;
        }



#hlinkEOR:hover > .menuhijo {
    font-style: normal;
    display: block !important;
}
